{
"name": "libreoffice-impress"
, "path": "/usr/bin/libreoffice"
, "default_params": ["--impress"]
, "paths": [
]
, "allow_files": true
, "xserver": {
	"enabled": true
	, "enable_tray": false
	, "tray_icon":"/usr/share/icons/gnome/scalable/apps/libreoffice-startcenter.svg"
	, "window_icon":"/usr/share/icons/gnome/scalable/apps/libreoffice-startcenter.svg"
}
, "networking":{
	"type":"empty"
}
, "whitelist": [
]
, "shared_folders": [
	"${XDG_DOCUMENTS_DIR}"
]
, "blacklist": [
]
, "environment": [
	{"name":"GTK_THEME", "value":"dapper-dark"}
	, {"name":"GTK2_RC_FILES", "value":"/usr/share/themes/dapper-dark/gtk-2.0/gtkrc"}
]
, "seccomp": {
	"mode":"blacklist"
	, "enforce": true
}
}
